# Routes
# This file defines all application routes (Higher priority routes first)
# ~~~~

# Home page
GET        /                    controllers.Application.index
GET        /graph/*id           controllers.Application.node(id)
GET        /test                controllers.Application.test
GET        /vertices            controllers.Application.vertex
GET        /edges               controllers.Application.edge
GET        /hallo               controllers.Application.hallo

#GET        /:v                   controllers.Application.node(v:String)


# Map static resources from the /public folder to the /assets URL path
GET        /assets/*file        controllers.Assets.at(path="/public", file)
GET        /public/*file        controllers.Assets.at(path="/public", file)
GET        /*file               controllers.Assets.at(path="/public", file)
